{
  "extends": "airbnb",
  "env": {
    "node": true,
    "mocha": true,
    "es6": true
  },
  "rules": {
    "no-underscore-dangle": 0,
    "max-len": [2, 110, 2, {
      "ignoreUrls": true,
      "ignoreComments": false
    }],
    "no-lonely-if" : 2,
    "curly": 2,
    "no-console": 2,
    "func-names": 0,
    "consistent-return": 0,
    "comma-dangle": 0,
    "space-before-function-paren": 0,
    "no-param-reassign": 0,
    "array-callback-return": 0,
    "no-restricted-syntax": 0,
    "no-use-before-define": 0,
    "wrap-iife": 0,
    "strict": 0,
    "no-prototype-builtins": 0,
    "import/no-dynamic-require": 0
  },
  "globals": {
    "angular": 1,
    "Promise": 1,
    "describe": 1,
    "beforeEach": 1,
    "inject": 1,
    "it": 1,
    "expect": 1,
    "fdescribe": 1,
    "fit": 1,
    "assert": 1
  }
}